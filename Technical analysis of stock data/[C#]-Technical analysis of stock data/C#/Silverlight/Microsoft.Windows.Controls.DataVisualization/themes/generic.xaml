										<Grid x:Name="SeriesContainer" />
										<Border BorderBrush="#FF919191" BorderThickness="1" />
									</Grid>
								</Grid>
							</Grid>
						</Grid>
					</Border>
				</ControlTemplate>
			</Setter.Value>
